export interface Anime {
  id: string;
  name: string;
  image: string;
  genre: string;
}

export interface Genre {
  id: string;
  name: string;
  animes: Anime[];
}

const createAnime = (id: string, name: string, image: string, genre: string): Anime => ({
  id,
  name,
  image,
  genre,
});

