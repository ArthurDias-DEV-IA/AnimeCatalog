    ],
  },
];

export const getAllAnimes = (): Anime[] => {
  return genres.flatMap((genre) => genre.animes);
};

export const searchAnimes = (query: string): Anime[] => {
  const lowerQuery = query.toLowerCase().trim();
  if (!lowerQuery) return [];
  
  return getAllAnimes().filter((anime) =>
    anime.name.toLowerCase().includes(lowerQuery)
  );
};
